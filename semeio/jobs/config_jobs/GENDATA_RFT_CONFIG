EXECUTABLE    gendata_rft

DEFAULT <ZONEMAP> ZONEMAP_NOT_PROVIDED
DEFAULT <CSVFILE> gendata_rft.csv
DEFAULT <OUTPUTDIRECTORY> .

ARGLIST -e <ECL_BASE> -t <PATH_TO_TRAJECTORY_FILES> -w <WELL_AND_TIME_FILE> -z <ZONEMAP> -c <CSVFILE> -o <OUTPUTDIRECTORY>

MIN_ARG    3
MAX_ARG    5
ARG_TYPE 0 STRING
ARG_TYPE 1 STRING
ARG_TYPE 2 STRING
ARG_TYPE 3 STRING
ARG_TYPE 4 STRING
ARG_TYPE 5 STRING
